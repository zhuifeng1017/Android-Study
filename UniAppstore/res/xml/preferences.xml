    <com.mappn.gfan.common.widget.GfanPreferenceCategory android:title="@string/pref_market_settings">
        <com.mappn.gfan.common.widget.GfanCheckBoxPreference android:title="@string/pref_market_app_update_notification" android:key="update_app_notification" android:summary="@string/pref_market_app_update_notification_summary" android:defaultValue="true" />
