        <com.mappn.gfan.common.widget.ClientDialogPreference android:persistent="false" android:title="@string/pref_search_settings_title" android:key="manual_clear_search_history" android:summary="@string/pref_search_settings_summary" android:dialogMessage="@string/pref_search_settings_dialog" />
